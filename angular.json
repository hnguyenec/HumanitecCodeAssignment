{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "programs": {
      "root": "apps/programs/",
      "sourceRoot": "apps/programs/src",
      "projectType": "application",
      "prefix": "humanitec",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/programs",
            "index": "apps/programs/src/index.html",
            "main": "apps/programs/src/main.ts",
            "polyfills": "apps/programs/src/polyfills.ts",
            "tsConfig": "apps/programs/tsconfig.app.json",
            "assets": [
              "apps/programs/src/favicon.ico",
              "apps/programs/src/assets"
            ],
            "styles": ["apps/programs/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/programs/src/environments/environment.ts",
                  "with": "apps/programs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "programs:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "programs:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "programs:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/programs/src/test.ts",
            "polyfills": "apps/programs/src/polyfills.ts",
            "tsConfig": "apps/programs/tsconfig.spec.json",
            "karmaConfig": "apps/programs/karma.conf.js",
            "styles": ["apps/programs/src/styles.scss"],
            "scripts": [],
            "assets": [
              "apps/programs/src/favicon.ico",
              "apps/programs/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/programs/tsconfig.app.json",
              "apps/programs/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "programs-e2e": {
      "root": "apps/programs-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/programs-e2e/protractor.conf.js",
            "devServerTarget": "programs:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "programs:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/programs-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "activities": {
      "root": "libs/activities",
      "sourceRoot": "libs/activities/src",
      "projectType": "library",
      "prefix": "humanitec",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/activities/src/test.ts",
            "tsConfig": "libs/activities/tsconfig.spec.json",
            "karmaConfig": "libs/activities/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/activities/tsconfig.lib.json",
              "libs/activities/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/schematics:application": {
      "style": "scss"
    },
    "@nrwl/schematics:library": {
      "style": "scss"
    }
  },
  "defaultProject": "programs"
}
